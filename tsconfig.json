{
  "compilerOptions": {
    "lib": [
      "es6",
      "es7",
      "es2015",
      "es2016",
      "es2017",
      "dom",
      "esnext.asynciterable"
    ],
    "target": "es5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "build/compiled",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "stripInternal": true,
    "pretty": true,
    "strictNullChecks": false,
    "declaration": false,
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitReturns": false
  },
  "files": ["./node_modules/web3/index.d.ts"],
  "include": ["./src", "./abi"],
  "exclude": ["./node_modules", "./build/compiled"]
}
